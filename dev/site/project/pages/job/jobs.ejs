


<div class="channel">
    <form id="form">
    
    <div class="channel_paging">

    </div>
    <div class="channel_list">
        <div class="channel_list_item">
            <div class="channel_list_item_left">
                
            </div>
            <div class="channel_list_item_right">
                <div class="channel_list_item_rigth_item">
                    <input type="button" id="remove" value="remove" onclick="remove_jobs();" disabled>
                </div>
            </div>
        </div>

        <input type="hidden" name="test">
        <% 
            if(jobs){
                jobs.forEach(job => { %>
                    <div class="channel_list_item">
                        <div class="channel_list_item_left">
                            <%=job.content %>
                        </div>
                        <div class="channel_list_item_right">
                            <div class="channel_list_item_rigth_item">
                                <a href="/job/job.html" onclick="$.set_cookie('jid', <%=job.job_id%>)">Edit</a>
                            </div>
                            <div class="channel_list_item_rigth_item">
                                <input type="checkbox" id="ids" name="ids" value="<%=job.job_id%>" onclick="$.element('remove').disabled = false;">
                            </div>
                        </div>
                    </div>
                <% });
            }
        %>
    </div>
    <div class="channel_paging">

    </div>

    </form>
</div>

