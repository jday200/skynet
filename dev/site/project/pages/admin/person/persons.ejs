


<div class="channel">
    <form id="form">
    
    <div class="channel_paging">

    </div>
    <div class="channel_list">
        <div class="channel_list_item">
            <div class="channel_list_item_left">
                
            </div>
            <div class="channel_list_item_right">
                <div class="channel_list_item_rigth_item">
                    <input type="button" id="remove" value="remove" onclick="remove_persons();" disabled>
                </div>
            </div>
        </div>

        <input type="hidden" name="test">
        <% 
            if(persons){
                persons.forEach(person => { %>
                    <div class="channel_list_item">
                        <div class="channel_list_item_left">
                            <%=person.username %>
                        </div>
                        <div class="channel_list_item_right">
                            <div class="channel_list_item_rigth_item">
                                <a href="/admin/person/profile.html" onclick="$.set_cookie('pid', <%=person.userid%>)">Edit</a>
                            </div>
                            <div class="channel_list_item_rigth_item">
                                <input type="checkbox" id="ids" name="ids" value="<%=person.userid%>" onclick="$.element('remove').disabled = false;">
                            </div>
                        </div>
                    </div>
                <% });
            }
        %>
    </div>
    <div class="channel_paging">

    </div>

    </form>
</div>

